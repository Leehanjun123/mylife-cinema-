{
  "version": 2,
  "builds": [
    {
      "src": "start-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/start-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "8000",
    "FRONTEND_URL": "https://lifecinema.site"
  },
}